{
  "name": "monorepo-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "api:dev": "nodemon --watch application/api --ignore applications/api/lowdb/* --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' applications/api/src/server.ts",
    "cypress:component": "./node_modules/.bin/cypress open --component",
    "lint": "./node_modules/.bin/eslint --cache --ext .jsx --ext .js --ext ts --ext tsx ./",
    "lint:fix": "./node_modules/.bin/eslint --cache --fix --ext .jsx --ext .js --ext ts --ext tsx ./ && prettier --write \"./packages/**/*.{ts,tsx,js,jsx,json,md}\"",
    "web:build": "tsc && vite --config ./applications/web/vite.config.ts build",
    "web:build:check": "tsc",
    "web:dev": "vite --config ./applications/web/vite.config.ts --port 4000",
    "web:preview": "vite --config ./applications/web/vite.config.ts preview",
    "test:unit": "env TS_NODE_PROJECT=\"tsconfig.json\" mocha --require ts-node/register --require tsconfig-paths/register --config .mocharc.js",
    "test:components": "rm -rf .nyc_output && ./node_modules/.bin/cypress run --component",
    "test:components:videos": "./node_modules/.bin/cypress run-ct -C ./cypress-videos.config.ts",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.12",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@hookform/resolvers": "2.9.7",
    "@vanilla-extract/css": "1.7.3",
    "axios": "0.27.2",
    "downshift": "6.1.9",
    "immer": "9.0.15",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-router-dom": "6.3.0",
    "react-virtuoso": "2.17.2",
    "uuid": "8.3.2",
    "zod": "3.18.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@cypress/code-coverage": "3.10.0",
    "@faker-js/faker": "7.5.0",
    "@loadable/component": "5.15.2",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/cypress": "8.0.3",
    "@types/axios": "0.14.0",
    "@types/chai": "4.3.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/loadable__component": "5.13.4",
    "@types/lodash": "4.14.184",
    "@types/mocha": "9.1.1",
